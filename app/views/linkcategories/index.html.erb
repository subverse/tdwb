<h1>Listing linkcategories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @linkcategories.each do |linkcategory| %>
  <tr>
    <td><%= linkcategory.name %></td>
    <td><%= linkcategory.info %></td>
    <td><%= link_to 'Show', linkcategory %></td>
    <td><%= link_to 'Edit', edit_linkcategory_path(linkcategory) %></td>
    <td><%= link_to 'Destroy', linkcategory, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Linkcategory', new_linkcategory_path %>
